// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Campground {
  id          String  @unique @default(cuid())
  title       String
  location    String
  image       String?
  price       Int
  description String

  @@map("campgrounds")
}
